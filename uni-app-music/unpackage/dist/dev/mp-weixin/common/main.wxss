.container {
	width: 100%;
	height: calc(100vh - 70px);
	overflow: hidden;
}
.container scroll-view {
	height: 100%;
}
.fixbg {
	width: 100%;
	height: 100vh;
	position: fixed;
	left: 0;
	top: 0;

	background-size: cover;
	background-position: center 0;
	-webkit-filter: blur(20px);
	        filter: blur(20px);
	-webkit-transform: scale(1.2);
	        transform: scale(1.2);
}

